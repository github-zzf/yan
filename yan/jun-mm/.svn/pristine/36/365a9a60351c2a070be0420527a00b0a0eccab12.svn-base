<!DOCTYPE html>
<html>
	<#include "/common/_admin.html">
	<script type="text/javascript" src="${ctx}/static/module/company/activityEnrollIsSigen.js?${v}"></script>
<body>
	<div class="main_body">
		<div class="main_title main_title_border">
			<span class="content_titel"><b class="top_icon">活动报名</b></span>
		</div>
		<div class="main_content main_change">
			<form class="xform search_box" id="form_activityEnrollIsSigen_search">
				<div class="xgroup">
					<label class="label">报名课程</label>
					<input type="text" name="activityName" class="xw2" autocomplete="off"/>
				</div>
				<div class="xgroup">
					<label class="label">课程开始日期</label>
					<input type="text" name="activityStartTime" id="f_is_activityStartTime" class="xw2 xdate" autocomplete="off"/>
				</div>
				<div class="xgroup">
					<label class="label">课程结束日期</label>
					<input type="text" name="activityEndTime" id="f_is_activityEndTime" class="xw2 xdate" autocomplete="off"/>
				</div>
				<div class="xgroup">
					<label class="label">报名用户</label>
					<input type="text" name="memberName" class="xw2" autocomplete="off"/>
				</div>
				<div class="xgroup">
					<label class="label">状态</label>
					<div class="xcombo" data-name="enrollStatus" id="f_status">
				  		<ul class="xtemplate">
					   		<li data-xv="{val}">{txt}</li>
					   	</ul>
				  	</div>
				</div>
				<div class="xgroup">
					<label class="label">是否签到</label>
					<div class="xcombo" data-name="isSign" id="f_isSigen">
				  		<ul class="xtemplate">
					   		<li data-xv="{val}">{txt}</li>
					   	</ul>
				  	</div>
				</div>
				<div class="clear"></div>
				<div class="xgroup" style="float:left; font-size: 18px; margin-top: 20px;" id="activityEnroll_name">
					<button type="button" class="xbtn" id="btn_search">查询</button>
					<button type="button" class="xbtn" id="btn_reset">重置</button>
					<button type="button" class="xbtn" id="btn_activityEnrollIsSigen_fanHui">返回</button>
				</div>
				<div class="clear"></div>
			</form>
			<div class="body_center">
		
				<div class="xtable" id="activityEnroll_activityEnrollIsSigen_xtable" style="width:1680px;">
					<table class="xhead">
						<tr>
							<th width="240px">操作</th>
							<th width="200px">活动名称</th>
							<th width="200px">活动开始时间</th>
							<th width="200px">活动结束时间</th>
							<th width="200px">报名用户</th>
							<th width="80px">状态</th>
							<th width="160px">报名日期</th>
							<th width="150px">等位编号</th>
							<th width="80px">是否签到</th>
							<th width="150px">签到时间</th>
							<th width="80px">支付状态</th>
						</tr>
					</table>
					<div class="xbody">
						<table>
							<tbody class="xtemplate">
								<tr>
									<td class="tc">{btnUpdate}{btnQuXiao}{btnQianDao}{btnPy}</td>
									<td>{activityName}</td>
									<td>{activityStartTime}</td>
									<td>{activityEndTime}</td>
									<td>{memberName}</td>
									<td>{statusTxt}</td>
									<td>{enrollTime}</td>
									<td>{waitNum}</td>
									<td>{isSigenTxt}</td>
									<td>{signTime}</td>
									<td>{payStateTxt}</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
			<#assign cls="activity_is_paging">
			<#include "/common/_paging.html">
		</div>
	</div>
	<#include "activityEnrollForm.html">
	<#include "activityEnrollPyForm.html">
	<#include "../link/link_member.html">
	
</body>
</html>