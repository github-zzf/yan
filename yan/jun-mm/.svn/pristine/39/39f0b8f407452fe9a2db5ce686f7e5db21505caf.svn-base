.jbox{
	position: relative;
	background: #f5f5f5;
	width: 100%;
    height: 100%;
}
.jleft{
	position: absolute;
	top:0;
	left:0;
	bottom:0;
	width:200px;
	background: #fff;
	overflow: auto;
}
.jleft .title{
	line-height: 40px;
	
}
.jbody{
	position: absolute;
	left : 210px;
	top:0;
	bottom:0;
	right:0;
	background: #fff;
    overflow: auto;
	
}
.tree_leaf{
	display:inline-block;
	width:20px;
	height:18px;
	margin-right:6px;
    vertical-align: middle;
	user-select:none;
	cursor:pointer;
	background: url("../img/fn.png") no-repeat right center;
	background-size: 12px 12px;
}
.tree_click{
	display:inline-block;
	width:20px;
	height:18px;
	margin-right:6px;
    vertical-align: middle;
	user-select:none;
	cursor:pointer;
	background: url("../img/fr.png") no-repeat right center;
	background-size: 10px 10px;
}
.tree-open{
	background: url("../img/fb.png") no-repeat right center;
	background-size: 12px 12px;
}
.tree-close{
}