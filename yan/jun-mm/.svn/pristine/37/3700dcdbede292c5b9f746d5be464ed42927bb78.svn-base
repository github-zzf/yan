<!DOCTYPE html>
<html>
	<#include "/common/_admin.html">
	<script type="text/javascript" src="${ctx}/static/module/sys/sysCache.js?${v}"></script>
<body>
	<div class="main_body">
		<div class="main_title main_title_border">
			<span class="content_titel"><b class="top_icon">缓存管理</b></span>
		</div>
		<div class="main_content main_change" style="width:400px; position:absolute;">
			<div class="body_center">
				<div class="xtable" id="sysCache_1_xtable" style="width:400px;">
					<table class="xhead">
						<tr>
							<th width="30px"></th>
							<th width="200px">缓存名称</th>
							<th width="80px">操作</th>
						</tr>
					</table>
					<div class="xbody">
						<table>
							<tbody class="xtemplate">
								<tr>
									<td>{index}</td>
									<td>{cacheName}</td>
									<td class="tc">{btnDelete}</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
		<div class="main_content main_change" style="width:400px; position:absolute; margin-left: 500px;">
			<div class="body_center">
				<div class="xtable" id="sysCache_2_xtable" style="width:400px;">
					<table class="xhead">
						<tr>
							<th width="30px"></th>
							<th width="200px">缓存键名</th>
							<th width="80px">操作</th>
						</tr>
					</table>
					<div class="xbody">
						<table>
							<tbody class="xtemplate">
								<tr>
									<td>{index}</td>
									<td>{cache}</td>
									<td class="tc">{btnDelete}</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
		<div style="width:200px; position:absolute; margin-left: 1000px; width:400px;">
			<div>
				<font>缓存名称:</font><br>
				<input type="text" id="cache_name" readonly="readonly" style="padding-left: 5px; width:300px; border: 1px solid #CACED6; height:30px; border-radius:5px;">
			</div>
			<div style="margin-top: 20px;">
				<font>缓存键名:</font><br>
				<input type="text" id="cache_key_name" readonly="readonly" style="padding-left: 5px; width:300px; border: 1px solid #CACED6; height:30px; border-radius:5px;">
			</div>
			<div style="margin-top: 20px;">
				<font>缓存内容:</font><br>
				<div id="cache_new" style="padding: 5px; width:300px; border: 1px solid #CACED6; height:600px; border-radius:5px; overflow-y: auto; white-space:normal; word-break:break-all; word-wrap:break-word;"></div>
			</div>
		</div>
	</div>
	
</body>
</html>