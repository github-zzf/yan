@charset "utf-8";

body, div, html, input, lable {
	margin: 0;
	padding: 0;
}

html {
	height: 100%;
}

body {
	width: 100%;
	height: 100%;
	font: "宋体", "Arial";
	font-size: 12px
}

input, button {
	outline: none;
}
.canvas3D {
	position:absolute;
	left:30%;
	top:50%;
	transform: translate(-50%, -70%);
	width:500px;
	height:auto;
	z-index: 2;
}

.log_bj {
	position:absolute;
	top:50%;
	transform: translate(0%, -45%);
	width:60%;
	height:500px;
	background: url("../img/dl_bg.png") center center no-repeat;
	background-size: auto 500px;
	z-index: 1;
}
.img_common {
	display: block;
	max-width: 100%;
	height: auto;
}

.log_box {
	width: 100%;
	height: 100%;
	background: url("../img/login_bg3.png") center center no-repeat;
	background-size: cover;
}

img {
	border: 0
}

p {
	margin: 0;
	padding: 0
}

li {
	list-style-type: none
}

.fixTable {
	width: 100%;
	table-layout: fixed
}

.clear {
	clear: both
}
.main-body {
	z-index: 999;
	width: 400px;
	height: 400px;
	top: 50%;
	left: 50%;
	position: absolute;
	background: url("../img/login_bg2.png") center center no-repeat;
	background-size: cover;
	overflow: hidden;
	margin-left: -200px;
	margin-top: -200px;
	border: 1px solid #eee7ec;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
}

.login_title{
	display: block;
	margin: 0 auto;
	padding: 40px 0 20px 0;
}

.main-body .hide {
	display: none !important
}

.no-border-radius-2 {
	border-radius: 12px 0 0 0 !important
}

.no-border-radius-4 {
	border-radius: 0 0 0 12px !important
}

.new-login {
	width: 428px;
	position: relative;
	float: left
}

.new-login .top {
	width: 428px;
	height: 117px;
	background-color: #0371d1;
	border-radius: 12px 12px 0 0;
	font-family: SourceHanSansCN-Regular;
	font-size: 30px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0;
	color: #fff;
	text-align: center;
	overflow: hidden;
	-webkit-transform: rotate(0deg);
	-moz-transform: rotate(0deg);
	-ms-transform: rotate(0deg);
	-o-transform: rotate(0deg);
	transform: rotate(0deg)
}
.new-login .title1 {
	margin-top: 20px;
}
.new-login .title2 {
	margin-top: 10px;
	font-size: 22px;
}

.new-login .top .bg1 {
	display: inline-block;
	width: 74px;
	height: 74px;
	background: #fff;
	opacity: .1;
	border-radius: 0 74px 0 0;
	position: absolute;
	left: 0;
	top: 43px
}

.new-login .top .bg2 {
	display: inline-block;
	width: 94px;
	height: 94px;
	background: #fff;
	opacity: .1;
	border-radius: 50%;
	position: absolute;
	right: -16px;
	top: -16px
}

.new-login .bottom {
	width: 428px;
	border-radius: 0 0 12px 12px;
	padding-bottom: 53px
}

.new-login .bottom .center {
	width: 288px;
	margin: 0 auto;
}

.new-login .bottom .center .item {
	border-bottom: 1px solid #ffffff;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	padding: 15px 0;
}

.new-login .bottom .center .item .validateImg {
	position: absolute;
	right: 36px;
	cursor: pointer
}

.new-login .bottom .center .errors {
	position: absolute;
	height: 26px;
	line-height: 25px;
	width: 252px;
	border: 1px solid #fdd3d3;
	bottom: 8px;
	padding-left: 33px;
	color: #e63333;
	display:none;
	background: url(../img/login_icon.png?v=201806191627) -104px -50px
		no-repeat #feefef
}

.new-login .bottom .center .item .icon {
	width: 30px;
	height: 30px
}
.new-login .bottom .center .item .icon-2 {
	background: url("../img/username.png") left center no-repeat;
	background-size: contain;
}

.new-login .bottom .center .item .icon-3 {
	background: url("../img/password.png") left center no-repeat;
	background-size: contain;
}

.new-login .bottom .center .item input {
	font-size: 16px;
	letter-spacing: 0;
	border: none;
	background: none;
	padding-left: 15px;
	color: #ffffff;
}

.new-login .bottom .tip {
	clear: both;
	width: 288px;
	margin: 0 auto;
	margin-top: 16px;
    display: flex;
    align-items: center;
    justify-content:flex-start ;
}

.new-login .bottom .tip .icon-nocheck {
	display: inline-block;
	width: 10px;
	height: 10px;
	border-radius: 2px;
	border: solid 1px #9abcda;
	position: relative;
	top: 2px;
	margin: 1px 8px 1px 1px;
	cursor: pointer
}

.new-login .bottom .tip .icon-check {
	margin: 0 7px 0 0;
	width: 14px;
	height: 14px;
	border: 0;
	background: url(../img/icon-login.png?v=201808271650) no-repeat -111px
		-48px
}

.new-login .bottom .tip .login-tip {
	font-size: 14px;
	color: #ff4c7e;
	cursor: pointer;
    margin-left: 8px;
}

.new-login .bottom .tip .no-pwd {
	font-stretch: normal;
	letter-spacing: 0;
	color: #1391ff;
	text-decoration: none;
	position: absolute;
	right: 62px
}

.new-login .bottom .deal {
	width: 288px;
	height: 56px;
	border-radius: 16px;
	margin: 24px auto 0;
	text-align: center;
	line-height: 50px;
	color: #fff;
	font-size: 16px;
	letter-spacing: 0;
	cursor: pointer;
	background: url("../img/login_bg.png") center center no-repeat;
	background-size: cover;
}

.qr-code {
	float: left;
	width: 387px;
	height: 518px;
	border-radius: 0 12px 12px 0;
	background-color: rgba(80, 130, 174, .3);
	position: relative
}

.qr-code .show-qr-code {
	width: 166px;
	height: 166px;
	background: url("../img/demo.png?v=201808271650") no-repeat;
	background-size: contain;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%, -50%);
	-moz-transform: translate(-50%, -50%);
	-ms-transform: translate(-50%, -50%);
	-o-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%)
}

.qr-code .show-qr-code .content {
	color: #fff;
	font-size: 14px;
	position: absolute;
	width: 210px;
	text-align: center;
	bottom: -43px;
	left: -22px;
	height: 13px;
	line-height: 13px;
	cursor: default
}

.foot {
	position: absolute;
	bottom: 28px;
	text-align: center;
	width: 100%
}

.foot a {
	text-decoration: none;
	color: #c7c7c7
}

/**
 * xmask
 */
.xmask_box {
	position: fixed;
	top: 0;
	left: 0;
	z-index: 9999;
	display: none;
	width: 100%;
	height: 100%;
	font-size: 16px;
}

.xmask_cover {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #000;
	opacity: 0.3;
	filter: alpha(opacity = 30);
}

.xmask_text {
	position: absolute;
	top: 50%;
	left: 50%;
	display: block;
	height: 60px;
	padding-right: 25px;
	line-height: 60px;
	border: 1px solid #bbb;
	background: url(../img/b.gif) 20px 15px no-repeat #fff;
	background-size: 30px auto;
	padding-left: 65px;
	color: #aaa;
}


input[type="checkbox"]{width:15px;height:15px;display: inline-block;text-align: center;vertical-align: middle; line-height: 15px;position: relative;}
input[type="checkbox"]::before{content: "";position: absolute;top: 0;left: 0;background: #f0e4ec;width: 100%;height: 100%;border: 1px solid #ffffff}
input[type="checkbox"]:checked::before{content: "\2713";background-color: #fff;position: absolute;top: 0;left: 0;width:100%;border: 1px solid #ff4c7e;color:#ff4c7e;font-size: 16px;font-weight: bold;}
input::-webkit-input-placeholder {
    color: #ffffff;
}
input::-moz-input-placeholder {
    color: #ffffff;
}
input::-ms-input-placeholder {
    color: #ffffff;
}